[
    {
        "environmentFiles": [
            {
                "type": "s3",
                "value": "<envfile>"
            }
        ],
        "entryPoint": [
            "entrypoint.sh"
        ],
        "portMappings": [
            {
                "hostPort": 0,
                "protocol": "tcp",
                "containerPort": 80
            },
            {
                "hostPort": 0,
                "protocol": "tcp",
                "containerPort": 443
            }
        ],
        "command": [
            "/usr/sbin/apache2ctl", "-D", "FOREGROUND"
        ],
        "memoryReservation": <memory-reservation>,
        "cpu": <cpu>,
        "memory": <memory>,
        "workingDirectory": "/var/www/html",
        "image": "<IMG>",
        "essential": true,
        "name": "<container-name>"
    }
]
